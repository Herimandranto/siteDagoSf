<!DOCTYPE html>
<html lang="fr">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="author" content="manCodeur">
    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <title>DagoAdmin | {% block title %}{% endblock %}</title>

    <!-- Favicons -->
    <link href="{{ asset('img/favicon.png') }}" rel="icon">
    <link href="{{ asset('img/apple-touch-icon.png') }}" rel="apple-touch-icon">

    <!-- Bootstrap core CSS -->
    <link href="{{ asset('admin/css/bootstrap.min.css') }}" rel="stylesheet">
    <!--external css-->
    <link href="{{ asset('admin/font-awesome/css/font-awesome.css') }}" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="{{ asset('admin/css/jquery.gritter.css') }}" />
    <!-- Custom styles for this template -->
    <link href="{{ asset('admin/css/style.css') }}" rel="stylesheet">
    <link href="{{ asset('admin/css/style-responsive.css') }}" rel="stylesheet">
    

    {% block stylesheets %}
          
    {% endblock %}
  </head>

  <body>
    <section id="container">
      <header style="padding:0px;top:0px" class="header black-bg">
        <div class="sidebar-toggle-box" style="margin-left:10px">
          <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
        </div>
        <!--logo start-->
        <a href="index.html" style="font-size:18px;" class="logo"><b>DAG<span>OUTSOURCING</span></b></a>
        <!--logo end-->
      
        <div class="top-menu">
          <ul class="nav pull-right top-menu">
            <li><a class="logout" href="{{ path('app_logout') }}">Logout</a></li>
          </ul>
        </div>
      </header>
      <!--header end-->


      <!-- **********************************************************************************************************************************************************
          MAIN SIDEBAR MENU
          ****************************************************************z******************************************************************************************* -->
      <!--sidebar start-->
          {% include 'admin/sidebar.html.twig' %}
      <!--sidebar end-->


      <!-- **********************************************************************************************************************************************************
          MAIN CONTENT
          *********************************************************************************************************************************************************** -->
      <!--main content start-->
      <section id="main-content">

      </section>
      <!--main content end-->

      {% block body %}{% endblock %}

      <!--footer start-->
      {% include 'admin/footer.html.twig' %}
      <!--footer end-->

    </section>


    <script src="{{ asset('admin/js/jquery.min.js') }}"></script>
    <script src="{{ asset('admin/js/bootstrap.min.js') }}"></script>
    <script class="include" type="text/javascript" src="{{ asset('admin/js/jquery.dcjqaccordion.2.7.js') }}"></script>
    <script src="{{ asset('admin/js/jquery.scrollTo.min.js') }}"></script>
    <script src="{{ asset('admin/js/Chart.js') }}"></script>
    
    <!--common script for all pages-->
    <script src="{{ asset('admin/js/common-scripts.js') }}"></script>
    <script type="text/javascript" src="{{ asset('admin/js/jquery.gritter.js') }}"></script>


    
    {% block javascripts %}
    {% endblock %}

  </body>

</html>
